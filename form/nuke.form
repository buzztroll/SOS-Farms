    <h2>Users</h2> 
    <PRE>

        <table border="1" class="users-table">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Address</th>
            <th>City</th>
        </tr>
        {% for user in users %}
        <tr>
            <td>
                {{user.firstname}}
            </td>
            <td>
                {{user.lastname}}
            </td>
            <td>
                {{user.email}}
            </td>
            <td>
                {{user.phone}}
            </td>
            <td>
                {{user.address}}
            </td>
            <td>
                {{user.city}}
            </td>
        </tr>
        {% endfor %}
        </table>

    </PRE>

